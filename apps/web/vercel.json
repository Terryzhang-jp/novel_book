{
  "buildCommand": "cd ../.. && pnpm install && cd apps/web && pnpm build",
  "installCommand": "echo 'Using custom build command'",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://novel.mintlify.dev/docs"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://novel.mintlify.dev/docs/:match*"
    }
  ]
}
